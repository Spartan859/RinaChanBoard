<view class='gallery-gallery {{className}}' style='{{style}}' ref='saveChildRef0'>
  <view class="weui-gallery {{show ? 'weui-gallery_show' : ''}} {{extClass}}">
    <view class='weui-gallery__info'>
      {{current+1}}/{{currentImgs.length}}
    </view>
    <custom-swiper class='weui-gallery__img__wrp' indicator-dots='{{false}}' current='{{current}}' autoplay='{{false}}' duration='{{500}}' onChange='change' onTap='antmoveAction' data-antmove-tap='hideGallery'>
      <custom-swiper-item a:for='{{currentImgs}}' a:key='{{index}}' ref-numbers='{{currentImgs}}'>
        <image mode='aspectFit' class='weui-gallery__img' src='{{item}}'>
        </image>
      </custom-swiper-item>
    </custom-swiper>
    <view class='weui-gallery__opr' a:if='{{showDelete}}'>
      <custom-navigator class='weui-gallery__del' onTap='antmoveAction' data-antmove-tap='deleteImg'>
        删除
      </custom-navigator>
    </view>
  </view>
</view>