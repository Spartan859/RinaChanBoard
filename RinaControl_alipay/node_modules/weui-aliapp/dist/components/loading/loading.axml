<view class='loading-loading {{className}}' style='{{style}}' ref='saveChildRef0'>
  <view class="wx_loading_view {{animated ? 'wx_loading_view__animated' : ''}} {{!show ? 'wx_loading_view__hide' : ''}} {{extClass}}" style="{{animated ? 'transition: height ' + duration + 'ms ease;' : ''}}" id='wx_loading_view'>
    <view a:if="{{type==='dot-white'}}" class='loading wx_dot_loading wx_dot_loading_white'>
    </view>
    <view a:elif="{{type==='dot-gray'}}" class='loading wx_dot_loading'>
    </view>
    <view a:elif="{{type==='circle'}}" class='weui-loadmore'>
      <view class='weui-loading'>
      </view>
      <view class='weui-loadmore__tips'>
        {{tips}}
      </view>
    </view>
  </view>
</view>