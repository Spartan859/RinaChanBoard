<view class='checkbox-checkbox {{className}}' style='{{style}}' ref='saveChildRef0'>
  <mp-cell has-footer='{{!multi}}' has-header='{{multi}}' ext-class="weui-check__label {{outerClass}} {{extClass}} {{!multi ? '^weui-cell_radio' : '^weui-cell_checkbox'}}" ext-hover-class='weui-active' onTap='checkedChange' ref='saveChildRef1'>
    <view slot='icon' a:if='{{multi}}'>
      <custom-checkbox value='{{value}}' checked='{{checked}}' disabled='{{disabled}}' color='{{color}}' class='weui-check'>
      </custom-checkbox>
      <icon class='weui-icon-checked'>
      </icon>
    </view>
    <view>
      {{label}}
    </view>
    <view slot='footer' a:if='{{!multi}}'>
      <radio value='{{value}}' checked='{{checked}}' disabled='{{disabled}}' color='{{color}}' class='weui-check'>
      </radio>
      <icon class='weui-icon-checked'>
      </icon>
    </view>
  </mp-cell>
</view>