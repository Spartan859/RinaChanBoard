<view class='slideview-slideview {{className}}' style='{{style}}' ref='saveChildRef0'>
  <import-sjs from='./slideview.sjs' name='handler'>
  </import-sjs>
  <view class="weui-slideview weui-movable-view {{icon ? 'weui-slideview_icon' : ''}} {{extClass}}" style='width: 100%;height: 100%;'>
    <view show='{{show}}' change:show='{{handler.showChange}}' rebounce='{{rebounce}}' change:rebounce='{{handler.rebounceChange}}' duration='{{duration}}' change:duration='{{handler.durationChange}}' change:disable='{{handler.disableChange}}' disable='{{disable}}' change:prop='{{handler.sizeReady}}' prop='{{size}}' class='weui-slideview__left left' style='width:100%;' onTransitionend='{{handler.transitionEnd}}' onTouchStart='antmoveAction' data-antmove-touchstart='{{handler.touchstart}}' onTouchMove='antmoveAction' data-antmove-touchmove='{{handler.touchmove}}' onTouchEnd='antmoveAction' data-antmove-touchend='{{handler.touchend}}'>
      <slot>
      </slot>
    </view>
    <view class='weui-slideview__right right'>
      <view class='weui-slideview__buttons' style='height:100%;width:100%;' a:if='{{buttons && buttons.length}}'>
        <view a:for='{{buttons}}' a:key='{{index}}' class='btn weui-slideview__btn__wrp {{item.className}} {{item.extClass}}' ref-numbers='{{buttons}}'>
          <view data-data='{{item.data}}' data-index='{{index}}' class='weui-slideview__btn' onTap='antmoveAction' data-antmove-tap='{{handler.hideButton}}'>
            <text a:if='{{!icon}}'>
              {{item.text}}            </text>            <image class='weui-slideview__btn__icon' a:else  src='{{item.src}}'>
            </image>
          </view>
        </view>
      </view>
    </view>
  </view>
</view>