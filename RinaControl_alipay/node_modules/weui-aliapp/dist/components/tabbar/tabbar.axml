<view class='tabbar-tabbar {{className}}' style='{{style}}' ref='saveChildRef0'>
  <view class='weui-tabbar {{extClass}}'>
    <view data-index='{{index}}' a:for='{{list}}' a:key='{{index}}' class="weui-tabbar__item {{index === current ? 'weui-bar__item_on' : ''}}" ref-numbers='{{list}}' onTap='antmoveAction' data-antmove-tap='tabChange'>
      <view style='position: relative;display:inline-block;'>
        <image src='{{current === index ? item.selectedIconPath : item.iconPath}}' class='weui-tabbar__icon'>
        </image>
        <mp-badge a:if='{{item.badge || item.dot}}' content='{{item.badge}}' style='position: absolute;top:-2px;left:calc(100% - 3px)' ref='saveChildRef1'>
        </mp-badge>
      </view>
      <view class='weui-tabbar__label'>
        {{item.text}}
      </view>
    </view>
  </view>
</view>