<view class='dialog-dialog {{className}}' style='{{style}}' ref='saveChildRef0'>
  <view class="weui-mask {{!show ? 'weui-mask_hidden' : '' }}" a:if='{{mask}}' onTap='antmoveAction' data-antmove-tap='close'>
  </view>
  <view a:if='{{show}}' class='weui-dialog__wrp {{extClass}}' onTap='antmoveAction' data-antmove-tap='close'>
    <view class='weui-dialog' catchTap='antmoveAction' data-antmove-tap='stopEvent'>
      <view class='weui-dialog__hd'>
        <view class='weui-dialog__title'>
          {{title}}          <slot name='title'>
          </slot>
        </view>
      </view>
      <view class='weui-dialog__bd'>
        <slot>
        </slot>
      </view>
      <view class='weui-dialog__ft'>
        <block a:if='{{buttons && buttons.length}}'>
          <view a:for='{{buttons}}' a:key='{{index}}' class='weui-dialog__btn {{item.className}} {{item.extClass}}' hover-class='weui-active' data-index='{{index}}' ref-numbers='{{buttons}}' onTap='antmoveAction' data-antmove-tap='buttonTap'>
            {{item.text}}
          </view>
        </block>
        <slot name='footer' a:else >
        </slot>
      </view>
    </view>
  </view>
</view>