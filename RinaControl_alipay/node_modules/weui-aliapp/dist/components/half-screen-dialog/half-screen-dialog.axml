<view class='half-screen-dialog-half-screen-dialog {{className}}' style='{{style}}' ref='saveChildRef0'>
  <view class="{{show ? 'weui-show' :'weui-hidden'}}">
    <view class='weui-mask init' a:if='{{mask}}' data-type='tap' onTap='antmoveAction' data-antmove-tap='close' catchTouchMove='antmoveAction' data-antmove-touchmove='onMaskMouseMove'>
    </view>
    <view class='weui-half-screen-dialog {{extClass}}'>
      <view class='weui-half-screen-dialog__hd'>
        <view a:if='{{closabled}}' class='weui-half-screen-dialog__hd__side' data-type='close' onTap='antmoveAction' data-antmove-tap='close'>
          <view class='weui-icon-btn weui-icon-btn_close' hover-class='weui-active'>
            关闭
          </view>
        </view>
        <view class='weui-half-screen-dialog__hd__main'>
          <block a:if='{{title}}'>
            <text class='weui-half-screen-dialog__title'>
              {{title}}            </text>            <text class='weui-half-screen-dialog__subtitle'>
              {{subTitle}}            </text>
          </block>          <block a:else >
            <view class='weui-half-screen-dialog__title'>
              <slot name='title'>
              </slot>
            </view>
          </block>
        </view>        <view class='weui-half-screen-dialog__hd__side'>
          <view class='weui-icon-btn weui-icon-btn_more' hover-class='weui-active'>
            更多
          </view>
        </view>
      </view>      <view class='weui-half-screen-dialog__bd'>
        <block a:if='{{desc}}'>
          <view class='weui-half-screen-dialog__desc'>
            {{desc}}
          </view>
          <view class='weui-half-screen-dialog__tips'>
            {{tips}}
          </view>
        </block>
        <slot name='desc' a:else >
        </slot>
      </view>
      <view class='weui-half-screen-dialog__ft'>
        <block a:if='{{buttons && buttons.length}}'>
          <btn a:for='{{buttons}}' a:key='{{index}}' type='{{item.type}}' class='weui-btn {{item.className}} {{item.type}}-style' data-index='{{index}}' ref-numbers='{{buttons}}' onTap='antmoveAction' data-antmove-tap='buttonTap'>
            {{item.text}}
          </btn>
        </block>
        <slot name='footer' a:else >
        </slot>
      </view>
    </view>
  </view>
</view>