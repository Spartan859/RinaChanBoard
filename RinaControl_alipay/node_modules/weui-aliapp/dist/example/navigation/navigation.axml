<view class='page' data-weui-theme='{{theme}}' ref='saveChildRef0'>
  <mp-navigation-bar loading='{{loading}}' show='{{show}}' animated='{{animated}}' color='{{color}}' background='{{background}}' title='UI组件库' back='{{true}}' ref='saveChildRef1'>
  </mp-navigation-bar>
  <view class='page__hd'>
    <view class='page__title'>
      Navigation
    </view>
    <view class='page__desc'>
      小程序自定义导航栏
    </view>
  </view>
  <view class='page__bd page__bd_spacing'>
    <btn antmove-class='antmove-weui-btn' class='weui-btn primary-style' type='primary' onTap='antmoveAction' data-antmove-tap='toggleLoading'>
      触发loading
    </btn>
    <btn antmove-class='antmove-weui-btn' class='weui-btn primary-style' type='primary' onTap='antmoveAction' data-antmove-tap='changeColor'>
      修改文字颜色
    </btn>
    <btn antmove-class='antmove-weui-btn' class='weui-btn primary-style' type='primary' onTap='antmoveAction' data-antmove-tap='changeBgColor'>
      修改背景颜色
    </btn>
    <btn antmove-class='antmove-weui-btn' class='weui-btn primary-style' type='primary' onTap='antmoveAction' data-antmove-tap='toggleShow'>
      显示 / 隐藏
    </btn>
    <btn antmove-class='antmove-weui-btn' class='weui-btn primary-style' type='primary' onTap='antmoveAction' data-antmove-tap='toggleAnimated'>
      设置显示 / 隐藏opacity动画
    </btn>
  </view>
</view>