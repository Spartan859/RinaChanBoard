<view class='page' data-weui-theme='{{theme}}' ref='saveChildRef0'>
  <view class='page__hd'>
    <view class='page__title'>
      Toptips
    </view>
  </view>
  <view class='page__bd'>
    <btn type='primary' disabled='{{disabled}}' class=' primary-style' onTap='antmoveAction' data-antmove-tap='showToptips'>
      显示Toptips
    </btn>
    <mp-toptips ext-class='top100' msg='距离顶部100px' type='error' show='{{show}}' ref='saveChildRef1'>
    </mp-toptips>
    <mp-toptips ext-class='top200' msg='距离顶部200px' type='info' show='{{show}}' ref='saveChildRef2'>
    </mp-toptips>
    <mp-toptips ext-class='top300' msg='距离顶部300px' type='success' show='{{show}}' ref='saveChildRef3'>
    </mp-toptips>
  </view>
</view>