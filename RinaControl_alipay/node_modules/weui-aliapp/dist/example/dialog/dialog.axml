<view class='page' data-weui-theme='{{theme}}' ref='saveChildRef0'>
  <view class='page__hd'>
    <view class='page__title'>
      Dialog
    </view>
    <view class='page__desc'>
      对话框
    </view>
  </view>
  <view class='page__bd'>
    <view class='weui-btn-area'>
      <btn antmove-class='antmove-weui-btn' class='weui-btn default-style' type='default' onTap='antmoveAction' data-antmove-tap='openConfirm'>
        确认取消按钮
      </btn>
      <btn antmove-class='antmove-weui-btn' class='weui-btn default-style' type='default' onTap='antmoveAction' data-antmove-tap='tapOneDialogButton'>
        只有确认按钮
      </btn>
    </view>
  </view>
  <mp-dialog title='test' show='{{dialogShow}}' buttons='{{buttons}}' onButtontap='tapDialogButton' ref='saveChildRef1'>
    <view>
      test content
    </view>
  </mp-dialog>
  <mp-dialog title='test1' show='{{showOneButtonDialog}}' buttons='{{oneButton}}' onButtontap='tapDialogButton' ref='saveChildRef2'>
    <view>
      test content1
    </view>
  </mp-dialog>
</view>