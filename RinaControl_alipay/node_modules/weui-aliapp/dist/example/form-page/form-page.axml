<view class='weui-form' ref='saveChildRef0'>
  <view class='weui-form__text-area'>
    <view class='weui-form__title'>
      表单标题
    </view>
    <view class='weui-form__desc'>
      表单描述示意
    </view>
  </view>
  <view class='weui-form__control-area'>
    <view class='weui-cells__group weui-cells__group_form'>
      <view class='weui-cells__title'>
        输入框
      </view>
      <view class='weui-cells weui-cells_form'>
        <view class='weui-cell'>
          <view class='weui-cell__hd'>
            <custom-label class='weui-label'>
              QQ号
            </custom-label>
          </view>
          <view class='weui-cell__bd'>
            <custom-input class='weui-input' type='number' pattern='[0-9]*' placeholder='请输入QQ号'>
            </custom-input>
          </view>
        </view>
      </view>
    </view>
  </view>
  <view class='weui-form__tips-area'>
    <custom-label for='weuiAgree' class='weui-agree'>
      <custom-input id='weuiAgree' type='checkbox' class='weui-agree__checkbox'>
      </custom-input>
      <text class='weui-agree__text'>
        阅读并同意        <a href='javascript:void(0);'>
          《相关条款》
        </a>
      </text>
    </custom-label>
  </view>  <view class='weui-form__opr-area'>
    <btn type='primary' class=' primary-style'>
      确定
    </btn>
  </view>
  <view class='weui-form__tips-area'>
    <view class='weui-form__tips'>
      点击下一步即表示      <custom-navigator href='javascript:;'>
        同意用户协议
      </custom-navigator>
    </view>
  </view>
  <view class='weui-form__extra-area'>
    <view class='weui-footer'>
      <view class='weui-footer__links'>
        <custom-navigator href='javascript:void(0);' class='weui-footer__link'>
          底部链接文本
        </custom-navigator>
      </view>
      <view class='weui-footer__text'>
        Copyright © 2008-2019 weui.io
      </view>
    </view>
  </view>
</view>