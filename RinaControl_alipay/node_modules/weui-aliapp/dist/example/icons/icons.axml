<view class='page' data-weui-theme='{{theme}}' ref='saveChildRef0'>
  <view class='page__hd'>
    <view class='page__title'>
      Icons
    </view>
    <view class='page__desc'>
      图标
    </view>
  </view>
  <view class='page__bd'>
    <view class='icon-box'>
      <view class='icon-box__title'>
        描边型
      </view>
      <view class='weui-grids'>
        <block a:for='{{iconList}}' a:key='{{*this}}' ref-numbers='{{iconList}}'>
          <custom-navigator url=" " class='weui-grid'>
            <mp-icon icon='{{item.icon}}' color='{{item.color}}' size='{{item.size}}' ref='saveChildRef1'>
            </mp-icon>
            <view class='weui-grid__label'>
              {{item.icon}}
            </view>
          </custom-navigator>
        </block>
      </view>
    </view>
    <view class='icon-box'>
      <view class='icon-box__title'>
        填充型
      </view>
      <view class='weui-grids'>
        <block a:for='{{iconList}}' a:key='{{*this}}' ref-numbers='{{iconList}}'>
          <custom-navigator url=" " class='weui-grid'>
            <mp-icon type='filled' icon='{{item.icon}}' color='{{item.color}}' size='{{item.size}}' ref='saveChildRef2'>
            </mp-icon>
            <view class='weui-grid__label'>
              {{item.icon}}
            </view>
          </custom-navigator>
        </block>
      </view>
    </view>
  </view>
</view>