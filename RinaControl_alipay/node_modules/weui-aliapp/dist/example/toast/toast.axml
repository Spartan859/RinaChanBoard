<view class='page' ref='saveChildRef0'>
  <view class='page__hd'>
    <view class='page__title'>
      Toast
    </view>
    <view class='page__desc'>
      弹出式提示，采用小程序原生的toast
    </view>
  </view>
  <view class='page__bd'>
    <view class='weui-btn-area'>
      <btn antmove-class='antmove-weui-btn' class='weui-btn default-style' type='default' onTap='antmoveAction' data-antmove-tap='openToast'>
        成功提示
      </btn>
      <btn antmove-class='antmove-weui-btn' class='weui-btn default-style' type='default' onTap='antmoveAction' data-antmove-tap='openLoading'>
        加载中提示
      </btn>
    </view>
  </view>
</view>