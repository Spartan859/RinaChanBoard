      <btn class='weui-btn default-style' type='default' disabled='true' plain='true'>
        按钮
      </btn>
      <btn class='weui-btn default-style' type='default' loading=" " plain='true'>
        按钮
      </btn>
      <btn class='weui-btn mini-btn primary-style mini-style' type='primary' size='mini'>
        按钮
      </btn>
      <btn class='weui-btn mini-btn default-style mini-style' type='default' size='mini'>
        按钮
      </btn>
      <btn class='weui-btn mini-btn warn-style mini-style' type='warn' size='mini'>
        按钮
      </btn>
