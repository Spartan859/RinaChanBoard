    <view class='page__title'>
      Slideview
    </view>
    <view class='page__desc'>
      左滑操作
    </view>
  </view>
  <view class='page__bd'>
    <view class='weui-cells'>
      <mp-slideview show='{{true}}' buttons='{{slideButtons}}' onButtontap='slideButtonTap' ref='saveChildRef1'>
        <mp-cell value='左滑可以删除' footer='说明文字' ref='saveChildRef2'>
        </mp-cell>
      </mp-slideview>
    </view>
    <view class='weui-slidecells'>
      <mp-slideview buttons='{{slideButtons}}' icon='{{true}}' onButtontap='slideButtonTap' ref='saveChildRef3'>
        <view class='weui-slidecell'>
          左滑可以删除（图标Button）
        </view>
      </mp-slideview>
    </view>
  </view>
</view>