<view class='page' data-weui-theme='{{theme}}' ref='saveChildRef0'>
  <mp-toptips msg='{{error}}' type='error' show='{{error}}' ref='saveChildRef1'>
  </mp-toptips>
  <mp-form-page title='表单结构' subtitle='展示表单页面的信息结构样式, 分别由头部区域/控件区域/提示区域/操作区域和底部信息区域组成。' ref='saveChildRef2'>
    <mp-form id='form' rules='{{rules}}' models='{{formData}}' ref='saveChildRef3'>
      <mp-cells title='单选列表项' ref='saveChildRef4'>
        <mp-checkbox-group prop='radio' multi='{{false}}' onChange='radioChange' ref='saveChildRef5'>
          <mp-checkbox a:for='{{radioItems}}' a:key='{{*this}}' label='{{item.name}}' value='{{item.value}}' checked='{{item.checked}}' ref-numbers='{{radioItems}}' ref='saveChildRef6'>
          </mp-checkbox>
        </mp-checkbox-group>
      </mp-cells>
      <mp-cells title='复选列表项' ref='saveChildRef7'>
        <mp-checkbox-group prop='checkbox' multi='{{true}}' onChange='checkboxChange' ref='saveChildRef8'>
          <mp-checkbox a:for='{{checkboxItems}}' a:key='{{*this}}' label='{{item.name}}' value='{{item.value}}' checked='{{item.checked}}' ref-numbers='{{checkboxItems}}' ref='saveChildRef9'>
          </mp-checkbox>
        </mp-checkbox-group>
      </mp-cells>
      <mp-cells title='表单' footer='底部说明文字底部说明文字' ref='saveChildRef10'>
        <mp-cell prop='name' title='姓名' ext-class=" " ref='saveChildRef11'>
          <custom-input data-field='name' class='weui-input' placeholder='请输入姓名' onInput='formInputChange'>
          </custom-input>
        </mp-cell>
        <mp-cell prop='qq' title='qq' ext-class=" " ref='saveChildRef12'>
          <custom-input data-field='qq' class='weui-input' placeholder='请输入qq' onInput='formInputChange'>
          </custom-input>
        </mp-cell>
        <mp-cell prop='mobile' title='手机号' ext-class=' weui-cell_vcode' ref='saveChildRef13'>
          <custom-input data-field='mobile' class='weui-input' placeholder='请输入手机号' onInput='formInputChange'>
          </custom-input>
          <btn slot='footer' type='default' class='weui-vcode-btn default-style'>
            获取验证码
          </btn>
        </mp-cell>
        <mp-cell prop='date' title='日期' ext-class=" " ref='saveChildRef14'>
          <picker data-field='date' mode='date' value='{{date}}' start='2015-09-01' end='2017-09-01' onChange='bindDateChange'>
            <view class='weui-input'>
              {{date}}
            </view>
          </picker>
        </mp-cell>
        <mp-cell prop='vcode' title='验证码' ext-class=' weui-cell_vcode' ref='saveChildRef15'>
          <custom-input data-field='vcode' class='weui-input' placeholder='请输入验证码' onInput='formInputChange'>
          </custom-input>
          <image slot='footer' class='weui-vcode-img' src='../images/vcode.jpg' style='width: 108px'>
          </image>
        </mp-cell>
      </mp-cells>
      <mp-cells title='提交后表单项报错' ref='saveChildRef16'>
        <mp-cell show-error=" " prop='idcard' title='卡号' ext-class=" " ref='saveChildRef17'>
          <custom-input data-field='idcard' class='weui-input' placeholder='请输入卡号' onInput='formInputChange'>
          </custom-input>
        </mp-cell>
      </mp-cells>
      <mp-cells title='开关' ref='saveChildRef18'>
        <mp-cell title='标题文字' ext-class=" " ref='saveChildRef19'>
          <switch slot='footer' checked=" ">
          </switch>
        </mp-cell>
      </mp-cells>
      <mp-cells title='文本框' ref='saveChildRef20'>
        <mp-cell title='标题文字' ext-class=" " ref='saveChildRef21'>
          <custom-input class='weui-input' placeholder='请输入文本'>
          </custom-input>
        </mp-cell>
      </mp-cells>
      <mp-cells title='文本域' ref='saveChildRef22'>
        <mp-cell has-header='{{false}}' has-footer='{{false}}' title=" " ext-class=" " ref='saveChildRef23'>
          <textarea class='weui-textarea' placeholder='请输入文本' style='height: 3.3em'>
          </textarea>
          <view class='weui-textarea-counter'>
            0/200
          </view>
        </mp-cell>
      </mp-cells>
      <mp-cells title='选择' ref='saveChildRef24'>
        <mp-cell ext-class='weui-cell_select weui-cell_select-before' ref='saveChildRef25'>
          <view slot='title' style='width: 105px'>
            <picker value='{{countryCodeIndex}}' range='{{countryCodes}}' onChange='bindCountryCodeChange'>
              <view class='weui-select'>
                {{countryCodes[countryCodeIndex]}}
              </view>
            </picker>
          </view>
          <custom-input class='weui-input' placeholder='请输入号码'>
          </custom-input>
        </mp-cell>
      </mp-cells>
      <mp-cells title='选择' ref='saveChildRef26'>
        <mp-cell has-header='{{false}}' ext-class='weui-cell_select' ref='saveChildRef27'>
          <picker value='{{accountIndex}}' range='{{accounts}}' onChange='bindAccountChange'>
            <view class='weui-select'>
              {{accounts[accountIndex]}}
            </view>
          </picker>
        </mp-cell>
        <mp-cell ext-class='weui-cell_select weui-cell_select-after' ref='saveChildRef28'>
          <view slot='title' class='weui-label'>
            国家/地区
          </view>
          <picker value='{{countryIndex}}' range='{{countries}}' onChange='bindCountryChange'>
            <view class='weui-select'>
              {{countries[countryIndex]}}
            </view>
          </picker>
        </mp-cell>
      </mp-cells>
    </mp-form>
    <checkbox-group slot='tips' onChange='bindAgreeChange'>
      <custom-label class='weui-agree'>
        <custom-checkbox class='weui-agree__checkbox-check'>
        </custom-checkbox>
        <text class='weui-agree__checkbox'>
        </text>        <view class='weui-agree__text'>
          阅读并同意          <custom-navigator>
            《相关条款》
          </custom-navigator>
        </view>
      </custom-label>
    </checkbox-group>    <view slot='button'>
      <btn antmove-class='antmove-weui-btn' class='weui-btn primary-style' type='primary' onTap='antmoveAction' data-antmove-tap='submitForm'>
        确定
      </btn>
    </view>
  </mp-form-page>
</view>