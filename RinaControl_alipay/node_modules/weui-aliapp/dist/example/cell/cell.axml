<view class='page' data-weui-theme='{{theme}}' ref='saveChildRef0'>
  <view class='page__hd'>
    <view class='page__title'>
      Cell
    </view>
    <view class='page__desc'>
      列表
    </view>
  </view>
  <view class='page__bd'>
    <mp-cells ext-class='my-cells' title='带说明的列表项' ref='saveChildRef1'>
      <mp-cell value='标题文字' footer='说明文字' ref='saveChildRef2'>
      </mp-cell>
      <mp-cell ref='saveChildRef3'>
        <view>
          标题文字（使用slot）
        </view>
        <view slot='footer'>
          说明文字
        </view>
      </mp-cell>
      <mp-slideview show='{{true}}' buttons='{{slideButtons}}' onButtontap='slideButtonTap' ref='saveChildRef4'>
        <mp-cell value='左滑可以删除' footer='说明文字' ref='saveChildRef5'>
        </mp-cell>
      </mp-slideview>
    </mp-cells>
    <mp-cells title='带图标、说明的列表项' footer='底部说明文字' ref='saveChildRef6'>
      <mp-cell value='标题文字' footer='说明文字' ref='saveChildRef7'>
        <image slot='icon' src='{{icon}}' style='margin-right: 16px;vertical-align: middle;width:20px; height: 20px;'>
        </image>
      </mp-cell>
      <mp-cell value='标题文字' footer='说明文字' ref='saveChildRef8'>
        <image slot='icon' src='{{icon}}' style='margin-right: 16px;vertical-align: middle;width:20px; height: 20px;'>
        </image>
      </mp-cell>
    </mp-cells>
    <mp-cells title='带跳转的列表项' ref='saveChildRef9'>
      <mp-cell hover=" " value='有hover效果，无跳转URL' footer='说明文字' ref='saveChildRef10'>
        <image slot='title' src='{{icon}}' style='margin-right: 16px;vertical-align: middle;width:20px; height: 20px;'>
        </image>
      </mp-cell>
      <mp-cell link=" " url='./cell' value='有跳转URL' footer='说明文字' ref='saveChildRef11'>
        <image slot='icon' src='{{icon}}' style='margin-right: 16px;vertical-align: middle;width:20px; height: 20px;'>
        </image>
      </mp-cell>
    </mp-cells>
  </view>
</view>